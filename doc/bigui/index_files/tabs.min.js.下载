!function(b,c,d,e){XF.TabsTag=XF.Element.newHandler({options:{},init:function(){this.headers=this.$target.find("> .titles > h3");this.bodies=this.$target.find("> .bodies > div");console.log(`Initialised with ${this.headers.length} headers and ${this.bodies.length} bodies.`);for(let a=0;a<this.headers.length;a++)b(this.headers[a]).click(()=>{this.showTab(a)})},showTab:function(a){this.headers.removeClass("active");this.bodies.removeClass("active");b(this.headers[a]).addClass("active");b(this.bodies[a]).addClass("active")}});
